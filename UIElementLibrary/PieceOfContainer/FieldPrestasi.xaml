        <local:MyTextBox x:Name="prestasi_txt" HorizontalAlignment="Left" Height="23" Margin="164,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="210"/>
        <local:MyButton x:Name="tambah_btn" Content="+" HorizontalAlignment="Left" Margin="374,10,0,0" VerticalAlignment="Top" Width="23" Click="OnClickTambah_btn" FontWeight="Bold" Height="23"/>
